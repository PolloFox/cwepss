{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
          "type": "package",
          "package": {
              "name": "d3/d3",
              "version": "v3.5.17",
              "type": "drupal-library",
              "dist": {
                  "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
                  "type": "zip"
              }
         }
       },
       {
           "type": "package",
           "package": {
               "name": "components/highlightjs",
               "version": "9.7.0",
               "type": "drupal-library",
               "dist": {
                   "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
                   "type": "zip"
               }
           }
       },
       {
          "type": "package",
          "package": {
            "name": "jackmoore/colorbox",
            "version": "1.6.4",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/jackmoore/colorbox/archive/master.zip",
              "type": "zip"
            }
          }
       },
       {
          "type": "package",
          "package": {
              "name": "woocommerce/flexslider",
              "version": "2.6.4",
              "type": "drupal-library",
              "dist": {
                  "url": "https://github.com/woocommerce/FlexSlider/archive/master.zip",
                  "type": "zip"
              }
          }
       },
        {
            "type": "package",
            "package": {
                "name": "malsup/jquery.cycle",
                "version": "3.0.3",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/malsup/cycle/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "briancherne/jquery.hoverIntent",
                "version": "1.9.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/briancherne/jquery-hoverIntent/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "douglascrockford/json2",
                "version": "2.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/douglascrockford/JSON-js/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "briancherne/jquery.hoverIntent": "^1.9",
        "components/highlightjs": "9.7.*",
        "composer/installers": "^1.4",
        "cweagans/composer-patches": "^1.6",
        "d3/d3": "3.5.*",
        "douglascrockford/json2": "^2.0",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/address": "^1.3",
        "drupal/admin_toolbar": "^1.20",
        "drupal/adminimal_theme": "^1.3",
	"drupal/adminimal_admin_toolbar": "^1.5",
        "drupal/amp": "^1.0",
        "drupal/bootstrap": "^3.6",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/bootstrap_library": "^1.9",
        "drupal/colorbox": "^1.4",
        "drupal/config_inspector": "^1.0@beta",
        "drupal/config_installer": "^1.5",
        "drupal/contact_emails": "^1.14",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/cookieconsent": "^1.3",
        "drupal/core": "~8.4",
        "drupal/ctools": "^3.0",
        "drupal/ds": "^3.1",
        "drupal/entityqueue": "^1.0@alpha",
        "drupal/externalauth": "^1.0",
        "drupal/features": "^3.5",
        "drupal/field_group": "^3.0@beta",
        "drupal/flexslider": "^2.0@beta",
        "drupal/geofield": "1.0.0-beta1",
        "drupal/geolocation": "^1.11",
        "drupal/google_analytics": "^2.2",
        "drupal/isbn": "1.x-dev",
        "drupal/ldap": "^3.0@beta",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.3",
        "drupal/metatag_google_scholar": "^1.0@beta",
        "drupal/migrate_source_csv": "^2.0",
        "drupal/migrate_tools": "^4.0@beta",
        "drupal/mollom": "^1.2",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/rules": "^3.0@alpha",
        "drupal/search_api": "^1.5",
        "drupal/shs": "^1.0@alpha",
        "drupal/simple_sitemap": "^2.10",
        "drupal/social_media": "^1.2",
        "drupal/social_media_links": "^2.6",
        "drupal/taxonomy_menu": "^3.3",
        "drupal/token": "^1.0",
        "drupal/twig_extensions": "^1.0",
        "drupal/twig_tweak": "^2.0",
        "drupal/video_embed_field": "^1.5",
        "drupal/views_bulk_operations": "^1.0@beta",
        "drupal/views_custom_cache_tag": "^1.0",
        "drupal/views_slideshow": "^4.5",
        "drupal/webform": "^5.0@beta",
        "drupal/zurb_foundation": "^6.0@alpha",
        "drush/drush": "~8.0|^9.0.0-beta8",
        "jackmoore/colorbox": "1.6.4",
        "malsup/jquery.cycle": "^3.0",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "woocommerce/flexslider": "2.6.4"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "drupal/console": "1.2",
        "drupal/devel": "^1.2",
        "drupal/examples": "1.x-dev",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8|~3.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
